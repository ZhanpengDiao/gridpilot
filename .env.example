# Amber Electric
AMBER_API_TOKEN=<your-amber-api-token>
AMBER_SITE_ID=<your-site-id>

# Battery Configuration
BATTERY_TYPE=tesla_powerwall
BATTERY_CAPACITY_KWH=13.5
BATTERY_MAX_CHARGE_KW=5.0
BATTERY_MAX_DISCHARGE_KW=5.0
BATTERY_ROUND_TRIP_EFFICIENCY=0.9
BATTERY_MIN_SOC_PERCENT=20
BATTERY_CYCLE_COST_CENTS=5

# Location (for weather/AEMO region)
LATITUDE=-33.8688
LONGITUDE=151.2093
NEM_REGION=NSW1

# Strategy Thresholds
CHARGE_PRICE_THRESHOLD_CENTS=8
SELL_PRICE_THRESHOLD_CENTS=25
SPIKE_RESERVE_SOC_PERCENT=40

# Decision Cycle
DECISION_INTERVAL_SECONDS=300

# Logging
LOG_LEVEL=INFO
